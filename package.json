{
  "name": "music-tag-native",
  "version": "0.0.0",
  "description": "Music tag reader / writter in Node.js / Browser, powered by napi-rs and lofty",
  "main": "index.js",
  "types": "index.d.ts",
  "browser": "browser.js",
  "repository": {
    "url": "git+ssh://git@github.com/subframe7536/music-tag-native.git",
    "type": "git"
  },
  "license": "MIT",
  "keywords": [
    "napi-rs",
    "NAPI",
    "N-API",
    "Rust",
    "node-addon",
    "node-addon-api",
    "music",
    "tags",
    "MP3",
    "M4A",
    "FLAC",
    "WAV",
    "ID3",
    "Vorbis"
  ],
  "files": [
    "index.d.ts",
    "index.js",
    "browser.js"
  ],
  "napi": {
    "binaryName": "music-tag-native",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-pc-windows-msvc",
      "x86_64-unknown-linux-musl",
      "aarch64-unknown-linux-gnu",
      "i686-pc-windows-msvc",
      "aarch64-linux-android",
      "aarch64-unknown-linux-musl",
      "aarch64-pc-windows-msvc",
      "wasm32-wasi-preview1-threads"
    ]
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "dev": "napi build --watch --target wasm32-wasip1-threads",
    "build": "napi build --platform --release --strip",
    "build:wasm": "napi build --release --target wasm32-wasip1-threads",
    "build:debug": "napi build --platform",
    "lint": "oxlint",
    "test": "vitest run",
    "play": "vite serve playground",
    "test:watch": "vitest",
    "prepublishOnly": "napi prepublish -t npm",
    "release": "pnpm build && pnpm test && bumpp"
  },
  "devDependencies": {
    "@emnapi/core": "^1.7.1",
    "@emnapi/runtime": "^1.7.1",
    "@napi-rs/cli": "^3.5.0",
    "@napi-rs/wasm-runtime": "^1.1.0",
    "@tybys/wasm-util": "^0.10.1",
    "@types/node": "^24.10.1",
    "bumpp": "^10.3.2",
    "emnapi": "^1.7.1",
    "oxlint": "^1.31.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  },
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
